{
  "name": "@agentris/db",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "db:push": "npx dotenv-cli -e ../../.env -- prisma db push",
    "db:generate": "npx dotenv-cli -e ../../.env -- prisma generate",
    "db:studio": "npx dotenv-cli -e ../../.env -- prisma studio"
  },
  "dependencies": {
    "@prisma/client": "5.8.1",
    "prisma": "5.8.1"
  },
  "devDependencies": {
    "dotenv-cli": "^10.0.0"
  }
}
