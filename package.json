{
  "name": "agentris",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=web",
    "dev:api": "turbo dev --filter=@agentris/api",
    "build": "turbo build",
    "test": "turbo test",
    "test:ui": "vitest --ui",
    "test:coverage": "turbo test -- --coverage",
    "test:unit": "turbo test:unit",
    "test:watch": "turbo test:watch",
    "test:web": "turbo test --filter=web",
    "test:api": "turbo test --filter=@agentris/api",
    "test:db": "turbo test --filter=@agentris/db",
    "test:shared": "turbo test --filter=@agentris/shared",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "db:push": "turbo db:push",
    "db:generate": "turbo db:generate",
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down",
    "docker:logs": "docker compose -f docker/docker-compose.yml logs -f",
    "prepare": "husky",
    "postinstall": "turbo db:generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.35.0",
    "@types/node": "^20.19.13",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
